      <sectiondef kind="func">
      <memberdef kind="function" id="namespacescatt__interp_1a64a9659890c173f40c4a55437cdec904" prot="private" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>subroutine</type>
        <definition>subroutine scatt_interp::dcreate_scatt_interpolant2d</definition>
        <argsstring>(numpts, x1, x2, func, method, rank)</argsstring>
        <name>dcreate_scatt_interpolant2d</name>
        <param>
          <type>numpts</type>
          <defname>numpts</defname>
        </param>
        <param>
          <type>x1</type>
          <defname>x1</defname>
        </param>
        <param>
          <type>x2</type>
          <defname>x2</defname>
        </param>
        <param>
          <type>func</type>
          <defname>func</defname>
        </param>
        <param>
